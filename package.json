{
  "name": "VISUAL PHOTONS",
  "version": "0.11.0",
  "description": "VISUAL PHOTONS",
  "descriptions": "Создание VISUAL PHOTONS на основе связки NWjs + react",
  "main": "main/index.html",
  "window": {
    "width": 1366,
    "height": 768,
    "icon": "main/atom_copy.ico"
  },
  "scripts": {
    "start": "webpack --mode development --watch",
    "nwjs-start": "run --x64 .",
    "nwjs-pack-win": "build --tasks win-x64 .",
    "nwjs-pack-linux": "build --tasks linux-x64 .",
    "nwjs-packager": "nwp",
    "nwjs-packager-run": "nwp -r"
  },
  "build": {
    "nwVersion": "stable"
  },
  "nwjs-packager": {
    "appFriendlyName": "VISUAL PHOTONS",
    "appWinIcon": "main/atom_copy.ico",
    "files": [
      "main/**",
      "Base/**",
      "Foto/**"
    ],
    "builds": {
      "linux": {
        "tar.gz": false
      },
      "osx": {
        "zip": false
      },
      "win": {
        "zip": false
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IlyaKantur/Fotonics-NWjs_React-.git"
  },
  "author": "Ilya Kantur",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/IlyaKantur/Fotonics-NWjs_React-/issues"
  },
  "homepage": "https://github.com/IlyaKantur/Fotonics-NWjs_React-#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "css-loader": "^5.0.1",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^4.5.0",
    "nw-builder": "^3.5.7",
    "nwjs-builder-phoenix": "^1.15.0",
    "reactstrap": "^8.7.1",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.2",
    "cubic-spline": "^3.0.3",
    "express": "^4.17.1",
    "fomantic-ui": "^2.8.8",
    "http-server": "^13.0.2",
    "nwjs": "^1.4.4",
    "nwjs-packager": "^1.5.0",
    "plotly.js": "^1.58.5",
    "plotly.js-basic-dist-min": "^2.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-plotly.js": "^2.5.1",
    "react-webcam": "^5.2.3"
  }
}
